forloop
let p=0;
for (let p=0; p<10; p++) {
     console.log("the value is fun");
     console.log("hii");
}
 Object:
 let Suma={
     Email:"suma123@gmail.com"
     Age:95
     school:"mpc",
     Class:"10th class",
     fruits:['apple', 'mango', 'orange',
'grapes']
};
console.log(suma.fruits[0],
suma.fruits[1];
console.log(suma.fruits[2];
console.log(suma.fruits[3];
console.log(suma.Age);
console.log(suma.email);
console.log(suma.class);

Switch:
let places= "Hindustan";
Switch (place) {
         case "Pune":
         console.log ("I love pune");
         break;
       
         Case "Delhi":
         console.log ("I love delhi");
         break;
         
         Case "Chennai":
         console.log ("I love Chennai");
         break;

         default:
         console.log("I like other places");
         break;

Variables:
let a=700;
let b=200;
console.log(a + b);
console.log(a - b);
console.log(a * b);
console.log(a / b);
console.log(a % b);

While loop:
let i = 0;
while (i < 10) {
    console.log ("the number is: " + i);
    i++;
}
